  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>HomeManagmentDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        HM_Expenses.id, HM_Expenses.amount, HM_Expenses.comment, HM_Expenses.ExpenseCategory_id, HM_Expenses.PurchesDate, 
                         HM_Expenses.paymentMethod, HM_Expenses.UserId, HM_Expenses.time_Stamp, HM_ExpensesCategories.CategoryHead, 
                         HM_ExpensesCategories.Category_desc
FROM            HM_Expenses INNER JOIN
                         HM_ExpensesCategories ON HM_Expenses.ExpenseCategory_id = HM_ExpensesCategories.ExpenseCategory_id
WHERE        (HM_Expenses.UserId = 4) 
and HM_Expenses.PurchesDate between @DateFrom and @DateTo
order by HM_Expenses.PurchesDate desc</CommandText>
      </Query>
      <Fields>
        <Field Name="Category_desc">
          <DataField>Category_desc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
