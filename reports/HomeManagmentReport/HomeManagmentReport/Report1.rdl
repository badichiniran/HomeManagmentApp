    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>HomeManagmentDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT         EC.CategoryHead,SUM(E.amount ) SumEmount
FROM            HM_Expenses E,HM_ExpensesCategories EC
WHERE       E.ExpenseCategory_id = EC.ExpenseCategory_id
			and  (E.UserId = 4)
and E.PurchesDate between @DateFrom and @DateTo
group by    EC.CategoryHead</CommandText>
      </Query>
      <Fields>
        <Field Name="CategoryHead">
          <DataField>CategoryHead</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SumEmount">
